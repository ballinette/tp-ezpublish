    <title>{{ metaTitle }}</title>
    {% endif %}
    <style>
    {% include '@KaliopStaticHtml/styles.css.twig' %}
    </style>
</head>
<body>

{% include '@KaliopStaticHtml/icons.svg.twig' %}

